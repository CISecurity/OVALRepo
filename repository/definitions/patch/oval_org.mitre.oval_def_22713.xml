    <reference ref_id="ELSA-2009:1060-02" ref_url="http://linux.oracle.com/errata/ELSA-2009-1060.html" source="VENDOR"/>
    <reference ref_id="CVE-2009-1373" ref_url="http://linux.oracle.com/cve/CVE-2009-1373.html" source="CVE"/>
