    <title>USN-1048-1 -- tomcat6 vulnerability</title>
    <affected family="unix">
      <platform>Ubuntu 10.10</platform>
      <platform>Ubuntu 9.10</platform>
      <platform>Ubuntu 10.04</platform>
      <product>tomcat6</product>
