  <metadata>
    <title>USN-809-1 -- gnutls12, gnutls13, gnutls26 vulnerabilities</title>
    <affected family="unix">
      <platform>Ubuntu 8.04</platform>
      <platform>Ubuntu 9.04</platform>
      <platform>Ubuntu 6.06</platform>
