    <title>USN-882-1 -- php5 vulnerabilities</title>
    <affected family="unix">
      <platform>Ubuntu 8.04</platform>
      <platform>Ubuntu 8.10</platform>
      <platform>Ubuntu 9.10</platform>
      <platform>Ubuntu 6.06</platform>
