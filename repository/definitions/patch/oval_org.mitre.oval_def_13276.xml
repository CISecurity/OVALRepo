    <oval-def:title>DSA-1857-1 camlimages -- integer overflow</oval-def:title>
    <oval-def:affected family="unix">
      <oval-def:platform>Debian 5.0</oval-def:platform>
      <oval-def:platform>Debian 4.0</oval-def:platform>
